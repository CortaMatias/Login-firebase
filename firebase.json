{
  "hosting": [
    {
      "public": "dist/login-firebase",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "Login-firebase",
      "source": ".",
      "frameworksBackend": {}
    }
  ],
  "firestore": {
    "rules": "y",
    "indexes": "firestore.indexes.json"
  }
}